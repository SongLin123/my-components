{
  "extends": "./tsconfig.base.json",
  "compilerOptions": {
    "target": "es2017",
    "composite": true,
    "jsx": "preserve",
    "lib": ["ES2018", "DOM", "DOM.Iterable"],
    "types": ["unplugin-vue-macros/macros-global"],
    "skipLibCheck": true,
    "moduleResolution": "NodeNext"
  },
  "include": ["packages", "env.d.ts"],
  "exclude": [
    "node_modules",
    "**/dist",
    "**/*.md"
  ]
}
